{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run serve\" ",
    "build-server": "babel server/lib -d server/dist",
    "build-server:w": "babel server/lib -w -d server/dist",
    "serve": "node server/dist/server.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "wp:w": "webpack --watch",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "@ngrx/store": "^1.3.3",
    "angular2": "2.0.0-beta.9",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.3",
    "express": "^4.13.4",
    "fs": "0.0.2",
    "path": "^0.12.7",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.24",
    "whatwg-fetch": "^0.11.0",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "@ngrx/devtools": "^1.3.3",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "node-sass": "^3.4.2",
    "path": "^0.12.7",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "typescript": "^1.8.7",
    "typings": "^0.7.5",
    "webpack": "^1.12.14"
  }
}
